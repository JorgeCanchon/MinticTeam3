@page
@model HospiEnCasa.App.Web.Pages.LogoutModel
@{}

<form method="post">
  <div class="container mt-5">
    <div class="row justify-content-center">
      <div class="col-sm-12 col-md-12 col-lg-4">
        <p>Esta seguro que quiere cerrar sesion?</p>
        <p>
          <button class="btn btn-primary" asp-page-handler="Logout">Si</button>
          <button class="btn btn-light ml-5 border-dark" asp-page-handler="DontLogout">No</button>
        </p>
      </div>
    </div>
  </div>
</form>